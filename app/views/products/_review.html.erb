<i class="fa fa-star fa-2x" aria-hidden="true"></i><i class="fa fa-star-o fa-2x" aria-hidden="true"></i><i class="fa fa-star-o fa-2x" aria-hidden="true"></i><i class="fa fa-star-o fa-2x" aria-hidden="true"></i><i class="fa fa-star-o fa-2x" aria-hidden="true"></i>
<% end %>
<br>
<br>
 <%= review.description %>
<br>
<br>
Review By: <%= review.user.first_name %>
<br>

<% if current_user %>
<% logged_in = current_user.id %>
<% end %>

<% if (review.user.id.to_i == logged_in) %>

<%= link_to "delete", [@product, @review], :method  => :delete %>
<% end %>
<hr>
</div>